{
  "name": "geometa-web",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky",
    "dev": "bunx --bun vite dev",
    "build": "svelte-kit sync && bunx --bun vite build",
    "preview": "bunx --bun vite vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "dotenv -e .env drizzle-kit migrate",
    "db:migrate:preview": "dotenv -e .env.preview.local -e .env drizzle-kit migrate",
    "db:migrate:prod": "dotenv -e .env.prod.local -e .env drizzle-kit migrate"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241004.0",
    "@fontsource/fira-mono": "^5.0.0",
    "@iconify/json": "^2.2.243",
    "@iconify/svelte": "^4.0.2",
    "@lucide/svelte": "^0.488.0",
    "@neoconfetti/svelte": "^2.0.0",
    "@sentry/vite-plugin": "^3.2.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "^5.1.0",
    "@sveltejs/enhanced-img": "^0.3.10",
    "@sveltejs/kit": "^2.7.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.7.0",
    "@zerodevx/svelte-toast": "^0.9.6",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.3.19",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.27.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "estree-walker": "^3.0.3",
    "flowbite": "^2.5.2",
    "flowbite-svelte": "^0.46.23",
    "globals": "^15.0.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.41",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.6",
    "prisma": "^5.19.0",
    "svelte": "^5.23.1",
    "svelte-check": "^4.0.5",
    "sveltekit-superforms": "^2.17.0",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0",
    "typescript-svelte-plugin": "^0.3.46",
    "unplugin-icons": "^0.19.2",
    "vite": "^5.4.4"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@sentry/sveltekit": "^9.5.0",
    "active-table": "^1.1.5",
    "arctic": "^1.9.2",
    "aws4fetch": "^1.0.20",
    "carta-md": "^4.6.1",
    "drizzle-orm": "^0.36.0",
    "drizzle-zod": "^0.5.1",
    "lucia": "^3.2.0",
    "mode-watcher": "^0.5.1",
    "postgres": "^3.4.5",
    "rehype-external-links": "^3.0.0",
    "rehype-sanitize": "^6.0.0",
    "runed": "^0.25.0",
    "zod": "^3.23.8"
  }
}
