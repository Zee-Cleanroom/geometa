{
  "name": "geometa-web",
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "dotenv -e .env drizzle-kit migrate",
    "db:migrate:preview": "dotenv -e .env.preview.local -e .env drizzle-kit migrate",
    "db:migrate:prod": "dotenv -e .env.prod.local -e .env drizzle-kit migrate"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.0",
    "@iconify/json": "^2.2.243",
    "@iconify/svelte": "^4.0.2",
    "@neoconfetti/svelte": "^2.0.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.7.2",
    "@sveltejs/enhanced-img": "^0.3.4",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^9.6.0",
    "@types/node": "^22.7.0",
    "@zerodevx/svelte-toast": "^0.9.6",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "flowbite": "^2.5.1",
    "flowbite-svelte": "^0.46.15",
    "globals": "^15.0.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.41",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prisma": "^5.19.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "sveltekit-superforms": "^2.17.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "unplugin-icons": "^0.19.2",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.8.1",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "active-table": "^1.1.5",
    "arctic": "^1.9.2",
    "aws4fetch": "^1.0.20",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "lucia": "^3.2.0",
    "zod": "^3.23.8"
  }
}
