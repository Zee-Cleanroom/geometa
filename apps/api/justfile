drizzle-generate:
  bunx drizzle-kit generate

drizzle-migrate:
  bunx drizzle-kit migrate

db-psql:
  psql -h localhost -U postgres geometa

alias db-shell := db-psql

environment := "local"
db-dump:
  ./utils/db/backup.sh {{environment}}

db-reset:
  psql -h localhost -U postgres postgres -f utils/db/reset.sql
